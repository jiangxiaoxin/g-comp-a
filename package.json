{
  "name": "g-comp-a",
  "version": "1.0.1",
  "private": false,
  "npm": {
    "tag": "beta"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "build:prod": "vite build --config vite.config.build.ts && npm run generate:types",
    "generate:types": "node generate/generate-types.js",
    "pub": "npm publish --tag=beta"
  },
  "license": "MIT",
  "main": "dist/g-comp-a.umd.js",
  "module": "dist/g-comp-a.es.js",
  "typings": "dist/gcompa.d.ts",
  "dependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.1.5",
    "vite": "^2.1.5",
    "vue-tsc": "^0.0.8",
    "@types/node": "^14.14.31",
    "fs-extra": "^9.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiangxiaoxin/g-comp-a.git"
  }
}
